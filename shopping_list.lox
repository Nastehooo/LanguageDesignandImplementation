// While loop
var i = 0;
while (i < 2) {
  print i;
  i = i + 1;
}

// If-else
var x = 4;
if (x > 3) {
  print "greater";
} else {
  print "less";
}

// Nested if-else
var y = 75;
if (y >= 70) {
  if (y < 80) {
    print "70-79";
  }
}


var i = 0;
while (i < 2) {
  var j = 0;
  while (j < 2) {
    print i * 10 + j;
    j = j + 1;
  }
  i = i + 1;
}
